<?xml version="1.0"?>
<!-- Launches all 5 robots -->
<launch>
<arg name="local_ip_address" default="127.0.0.1"/>
<arg name="remote_ip_address" default="172.16.33.100"/>

        <include file="$(find ros5dpo_bringup)/launch/bringup.launch">
            <arg name="agent_number" value="1"/>
            <arg name="local_ip_address" value="$(arg local_ip_address)"/>
            <arg name="remote_ip_address" value="$(arg remote_ip_address)"/>
            <arg name="receive_port" value="5002"/>
            <arg name="send_port" value="5003"/>
        </include>

        <include file="$(find ros5dpo_bringup)/launch/bringup.launch">
            <arg name="agent_number" value="2"/>
            <arg name="local_ip_address" value="$(arg local_ip_address)"/>
            <arg name="remote_ip_address" value="$(arg remote_ip_address)"/>
            <arg name="receive_port" value="5012"/>
            <arg name="send_port" value="5013"/>
        </include>

        <include file="$(find ros5dpo_bringup)/launch/bringup.launch">
            <arg name="agent_number" value="3"/>
            <arg name="local_ip_address" value="$(arg local_ip_address)"/>
            <arg name="remote_ip_address" value="$(arg remote_ip_address)"/>
            <arg name="receive_port" value="5022"/>
            <arg name="send_port" value="5023"/>
        </include>

        <include file="$(find ros5dpo_bringup)/launch/bringup.launch">
            <arg name="agent_number" value="4"/>
            <arg name="local_ip_address" value="$(arg local_ip_address)"/>
            <arg name="remote_ip_address" value="$(arg remote_ip_address)"/>
            <arg name="receive_port" value="5032"/>
            <arg name="send_port" value="5033"/>
        </include>

        <include file="$(find ros5dpo_bringup)/launch/bringup.launch">
            <arg name="agent_number" value="5"/>
            <arg name="local_ip_address" value="$(arg local_ip_address)"/>
            <arg name="remote_ip_address" value="$(arg remote_ip_address)"/>
            <arg name="receive_port" value="5042"/>
            <arg name="send_port" value="5043"/>
        </include>

        <include file="$(find ros5dpo_bringup)/launch/visualize.launch">
        </include>

</launch>	
